{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {      
      "src": "resume.html",
      "use": "@vercel/static"
    },   
    },
    {
      "src": "/script.js",
      "dest": "/script.js"
    },    
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

